<%= search_form_for(@searchs, url: admin_products_path, method: "get") do |f| %>
  <%= f.label :name_cont, t(".name") %>
  <%= f.text_field :name_cont, value: params[:name_cont], class: "form-control" %>
  <%= f.label :price_gteq, t(".price_from") %>
  <%= f.text_field :price_gteq, value: params[:price_gteq], class: "form-control" %>
  <%= f.label :price_lteq, t(".price_to") %>
  <%= f.text_field :price_lteq, value: params[:price_lteq], class: "form-control" %>
  <%= f.label :category_id_eq, "Catgory"%>
  <%= f.select :category_id_eq, category_options, {}, {prompt: params[:category_id_eq], class: "form-control"} %>
  <%= f.submit "Search", class: "btn btn-success" %>
<% end %>
